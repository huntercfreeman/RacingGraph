@typeparam ItemType
@if (RacingGraphObject != null)
{
    <div style="@RacingGraphObject.DisplayCSS position: inherit; top:@(RacingGraphObject.Top)px; display:flex;margin-bottom: 2%;box-sizing: border-box; height: 20%; width: 100%;" class="dm2bdRacingGraphBarContainer">
        <div style="width:6%;" class="dm2bdRacingGraphImageContainer">
            <img src="@RacingGraphObject.ImageURLSelector(RacingGraphObject.Item)" style="height: 100%;" />
        </div>
        <div style="display: flex; justify-content: flex-end; align-items: center; padding-right: 10px; background-color: beige; height: 100%; width:@(RacingGraphObject.ScoreListSelector(RacingGraphObject.Item).ElementAt(RacingGraphObject.DateIndex) / RacingGraphObject.MaxValue * 60)%; max-width: 60%;" class="dm2bdRacingGraphBar">
            @RacingGraphObject.NameSelector(RacingGraphObject.Item)
        </div>
        <div style="padding-left: 10px; display: flex; align-items: center;" class="dm2bdRacingGraphScore">
            @RacingGraphObject.ScoreListSelector(RacingGraphObject.Item).ElementAt(RacingGraphObject.DateIndex)
        </div>
    </div>
}